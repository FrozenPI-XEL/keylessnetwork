{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "outDir": "./build",
    "rootDir": "./src",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "include": ["./src/**/*"],
    "exclude": ["node_modules"]
  }
}